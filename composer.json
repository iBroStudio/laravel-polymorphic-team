{
    "name": "ibrostudio/polymorphic-team",
    "version": "1.0.0",
    "description": "This is my package polymorphic-team",
    "keywords": [
        "iBroStudio",
        "laravel",
        "polymorphic-team"
    ],
    "homepage": "https://github.com/ibrostudio/polymorphic-team",
    "license": "MIT",
    "authors": [
        {
            "name": "iBroStudio",
            "email": "hello@ibro.studio"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "mpociot/teamwork": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "tightenco/parental": "^1.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IBroStudio\\PolymorphicTeam\\": "src/",
            "IBroStudio\\PolymorphicTeam\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IBroStudio\\PolymorphicTeam\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-polymorphic-team --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IBroStudio\\PolymorphicTeam\\PolymorphicTeamServiceProvider"
            ],
            "aliases": {
                "PolymorphicTeam": "IBroStudio\\PolymorphicTeam\\Facades\\PolymorphicTeam"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
